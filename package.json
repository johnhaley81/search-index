{
  "name": "search-index",
  "description": "A text search index module for Node.js. Search-index allows applications to add, delete and retrieve documents from a corpus. Retrieved documents are ordered by tf-idf relevance, filtering on metadata, and field weighting",
  "version": "0.6.15",
  "homepage": "https://github.com/fergiemcdowall/search-index",
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT",
  "dependencies": {
    "bunyan": "1.5.1",
    "leveldown": "^1.4.3",
    "levelup": "1.2.1",
    "lodash": "3.10.1",
    "search-index-adder": "0.0.3",
    "search-index-deleter": "0.0.1",
    "search-index-getter": "0.0.1",
    "search-index-matcher": "0.0.1",
    "search-index-replicator": "johnhaley81/search-index-replicator#json-stream-fix",
    "search-index-searcher": "0.0.1",
    "term-vector": "0.0.12"
  },
  "devDependencies": {
    "brfs": "1.4.0",
    "browserify": "11.2.0",
    "mocha": "2.3.3",
    "reuters-21578-json": "0.0.6",
    "should": "7.1.0",
    "stream-buffers": "2.2.0",
    "world-bank-dataset": "0.0.1"
